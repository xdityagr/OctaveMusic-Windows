         <property name="styleSheet">
          <string notr="true">QScrollArea {
		border:none; color:white;
        border: none;
        background-color: transparent;
    }

    /* Vertical Scrollbar */
    QScrollBar:vertical {
        border: none;
        background: rgba(255, 255, 255, 30); /* Scrollbar background */
        width: 8px; /* Width of the scrollbar */
        margin: 0px 0px 0px 0px; /* Space around the scrollbar */
        border-radius: 4px; /* Rounded scrollbar */
    }

    /* Horizontal Scrollbar */
    QScrollBar:horizontal {
        border: none;
        background: rgba(255, 255, 255, 30); /* Scrollbar background */
        height: 8px; /* Height of the scrollbar */
        margin: 0px 0px 0px 0px; /* Space around the scrollbar */
        border-radius: 4px; /* Rounded scrollbar */
    }

    /* Handle for vertical scrollbar */
    QScrollBar::handle:vertical {
        background: rgba(255, 255, 255, 100); /* Scrollbar handle color */
        min-height: 20px; /* Minimum height of the handle */
        border-radius: 4px; /* Rounded handle */
    }

    /* Handle for horizontal scrollbar */
    QScrollBar::handle:horizontal {
        background: rgba(255, 255, 255, 100); /* Scrollbar handle color */
        min-width: 20px; /* Minimum width of the handle */
        border-radius: 4px; /* Rounded handle */
    }

    /* Handle when hovered */
    QScrollBar::handle:vertical:hover,
    QScrollBar::handle:horizontal:hover {
        background: rgba(255, 255, 255, 150); /* Slightly darker when hovered */
    }

    /* Handle when pressed */
    QScrollBar::handle:vertical:pressed,
    QScrollBar::handle:horizontal:pressed {
        background: rgba(255, 255, 255, 180); /* Even darker when pressed */
    }

    /* Add arrows for the scrollbars */
    QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical,
    QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
        border: none;
        background: none;
        width: 0px;
        height: 0px;
    }

    /* Corner between vertical and horizontal scrollbars */
    QScrollBar::corner {
        background: transparent; /* Hide corner */
    }</string>
