   <string notr="true">/* VERTICAL SCROLLBAR */
QScrollBar:vertical {
    border: none;
    background: rgba(255, 255, 255, 100); /* Slight tint of white background */
    width: 10px; /* Reduced width */
    margin: 15px 0;
    border-radius: 5px;
}

/* HANDLE BAR VERTICAL */
QScrollBar::handle:vertical {    
    background-color: white; /* White handle */
    min-height: 30px;
    border-radius:5px;
}
QScrollBar::handle:vertical:hover {    
    background-color: rgb(255, 255, 255); /* Light gray hover color */
}
QScrollBar::handle:vertical:pressed {    
    background-color: rgba(255, 255, 255, 100); /* Semi-transparent pressed color */
}

/* BTN TOP - SCROLLBAR */
QScrollBar::sub-line:vertical {
    border: none;
    background-color: rgba(255, 255, 255, 100); /* Slight tint of white background */
    height: 10px;
    width: 10px; /* Rounded button */
    border-radius: 5px; /* Completely rounded */
    subcontrol-position: top;
    subcontrol-origin: margin;
    background-image: url('path_to_up_arrow_icon.png'); /* Icon URL */
    background-repeat: no-repeat;
    background-position: center;
}
QScrollBar::sub-line:vertical:hover {    
    background-color: rgb(255,255,255); /* Light gray hover color */
}
QScrollBar::sub-line:vertical:pressed {    
    background-color: rgb(255, 255, 255); /* Semi-transparent pressed color */
}

/* BTN BOTTOM - SCROLLBAR */
QScrollBar::add-line:vertical {
    border: none;
    background-color: rgba(255, 255, 255, 100); /* Slight tint of white background */
    height: 10px;
    width: 10px; /* Rounded button */
    border-radius: 5px; /* Completely rounded */
    subcontrol-position: bottom;
    subcontrol-origin: margin;
    background-image: url('path_to_down_arrow_icon.png'); /* Icon URL */
    background-repeat: no-repeat;
    background-position: center;
}
QScrollBar::add-line:vertical:hover {    
    background-color: rgb(255,255,255); /* Light gray hover color */
}
QScrollBar::add-line:vertical:pressed {    
    background-color: rgba(255, 255, 255, 100); /* Semi-transparent pressed color */
}

/* RESET ARROW */
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
    background: none;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}




/* HORIZONTAL SCROLLBAR */
QScrollBar:horizontal {
    border: none;
    background-color: rgba(255, 255, 255, 80); /* More transparent background */
    height: 6px; /* Reduced height */
    margin: 0 15px;
    border-radius: 3px;
}

/* HANDLE BAR HORIZONTAL */
QScrollBar::handle:horizontal {    
    background-color: rgba(255,255,255,80); /* White handle */
    min-width: 30px;
    border-radius: 3px;
}
QScrollBar::handle:horizontal:hover {    
    background-color: rgba(255,255,255,80); /* White handle */
}
QScrollBar::handle:horizontal:pressed {    
    background-color: rgba(255,255,255,80); /* White handle */
}

/* BTN LEFT - SCROLLBAR */
QScrollBar::sub-line:horizontal {
    border: none;
    background-color: rgba(255, 255, 255, 0); /* Slight tint of white background */
    width: 6px; /* Rounded button */
    height: 6px;
    border-radius: 3px; /* Completely rounded */
    subcontrol-position: left;
    subcontrol-origin: margin;
  /**  background-image: url('path_to_left_arrow_icon.png');  Icon URL */
    background-repeat: no-repeat;
    background-position: center;
}
QScrollBar::sub-line:horizontal:hover {    
    background-color: rgba(255,255,255, 0); /* Light gray hover color */
}
QScrollBar::sub-line:horizontal:pressed {    
    background-color: rgba(255, 255, 255, 0); /* Semi-transparent pressed color */
}

/* BTN RIGHT - SCROLLBAR */
QScrollBar::add-line:horizontal {
    border: none;
    background-color: rgba(255, 255, 255, 0); /* Slight tint of white background */
    width: 6px; /* Rounded button */
    height: 6px;
    border-radius: 3px; /* Completely rounded */
